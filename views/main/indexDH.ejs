<h2>Channel: <%= channel.name %></h2>


<div class="row">
  <ul class="search-items">
    <% var metrics = channelMetrics %>
    <% search_terms.forEach(function(term,idx) { %>

      <div class="col-md-3 col-lg-3 search-item">
        <li>
        <h4><%= term.term %></h4>
        <div class="termImage img-responsive"><img src="<%= term.image_url %>"></div>
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            NEGATIVE-TO-POSITIVE GRAPH
          </div>
        </div>
        <div class="row">
          <div class="tweet-num">reTweet total: <%= channelMetrics[idx].retweet_total %></div>
          <div class="tweet-num">favorite total: <%= channelMetrics[idx].favorite_total %></div>
        </div>
        </li>
      </div>
    <% }); %>
  </ul>
</div>

<script type="text/javascript">
  var search_terms = "<%- search_terms %>";
</script>


<div class="well">
  metrics:  <%= JSON.stringify(channelMetrics) %>
</div>